package co.com.sigepro.entidades;

import java.io.Serializable;
import javax.persistence.*;


/**
 * The persistent class for the actividades_herramientas database table.
 * 
 */
@Entity
@Table(name="actividades_herramientas")
public class ActividadHerramienta implements Serializable {
	private static final long serialVersionUID = 1L;

	@EmbeddedId
	private ActividadHerramientaPK id;



    public ActividadHerramienta() {
    }

	public ActividadHerramientaPK getId() {
		return this.id;
	}

	public void setId(ActividadHerramientaPK id) {
		this.id = id;
	}
	
	
}